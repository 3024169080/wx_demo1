/* pages/main/main.wxss */
swiper{
  height:350rpx;
  border-bottom:1rpx solid #e4e4e4;
}
.body view{
  background: #fff;
}
.types{
  display: flex;
  flex-wrap:wrap;  
}
.type{
  width: 25%;
  display: flex;
  flex-direction:column;
  align-items: center;
}
.type text{
  font-size:24rpx;
}
.type image{
  width: 84rpx;
  height:84rpx;
}
.goodslist{
  margin-top: 20rpx;  
  border-top:1rpx solid #e4e4e4;
}
.goods-tit{ 
  padding:10rpx 0 0 20rpx;
  line-height:50rpx;  
  display: flex;
  align-items: center;
  color: #666;
}
.goods-tit image{
  margin-right:10rpx;
  width: 30rpx;
  height:30rpx;
}


/* .goods-info{
  padding:20rpx;
} */
.info{
  padding:40rpx 20rpx;
  display:flex;
  justify-content: space-between;
  border-bottom:1rpx solid #e4e4e4;  
}
.longitude{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:0 20rpx;
  width: 100%;
  height:80rpx;
  background: #3190e8!important;
  color: #fff;
}
.selectaddr{
  font-size:30rpx;
  line-height: 80rpx;
}
.info-cont{
  width: 80%;  
  height:150rpx;
}
.info-cont text{
  font-size:22rpx;
  color: #666;
}
.info-img{
  width:126rpx;
  height:126rpx;
  margin-right:20rpx;
  flex-shrink: 0;
}
.info-flex{  
  display:flex;
  justify-content: space-between;
}
.info-flex:nth-child(2){
  margin:10rpx 0;
}

.ctop-tit text:nth-child(1){
  display:inline-block;
  font-size:22rpx;
  background: #ffd930;
  padding:0 8rpx;
  border-radius:5rpx;
  margin-right:10rpx;
}
.ctop-tit text:nth-child(2){
  font-size:35rpx;
  font-weight:bold;
}
.ctop-tag text{
  border: 1rpx solid #e5e5e5;
  border-radius:5rpx;
  color: #666;
  padding:0 5rpx;
  margin:0 2rpx;
}
.cmiddle-left text{
  margin:0 10rpx;
}
.cmiddle-right text{
  padding:0 5rpx;
  border-radius:5rpx;
  margin:0 5rpx;
}
.cbottom-right text:nth-child(2){
  color: #3190e8;
}
.shop-star {
  display: inline-block;
  margin-right: 10rpx;
  position: relative;
  height: 16rpx;
  width: 100rpx;
}

.shop-star image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100rpx;
  height: 16rpx;
}
.shop-star .star {  
  position: absolute;
  top: 0;
  left: 0;
  width: 100rpx;
  height: 16rpx;
  background: url("data:image/png;base64,") no-repeat left center / 100rpx 16rpx;
}

